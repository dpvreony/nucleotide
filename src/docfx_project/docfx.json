{
  "metadata": [
    {
      "src": [
        {
            "files": [
                "**.csproj"
            ],
            "src" :  ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "DocFx.Plugins.PlantUml/template"
        ],
        "postProcessors": [],
        "markdownEngineProperties": {
            // "plantUml.localGraphvizDotPath": "C:/Program Files (x86)/Graphviz2.38/bin/dot.exe",
            "plantUml.localPlantUmlPath": "../../tools/plantuml.jar",
            //"plantUml.outputFormat": "png",
            "plantUml.remoteUrl": "http://www.plantuml.com/plantuml/",
            "plantUml.renderingMode": "remote"
        },
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
