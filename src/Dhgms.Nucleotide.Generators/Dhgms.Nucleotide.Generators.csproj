<Project Sdk="Microsoft.NET.Sdk">
  <!--<Sdk Name="CodeGeneration.Roslyn.Plugin.Sdk" Version="0.7.63" />-->
  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.1;netcoreapp5.0</TargetFrameworks>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <LangVersion>9</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="bin/$(Configuration)/**/*.dll" Pack="true" PackagePath="build" KeepDuplicates="false" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="3.3.2">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.8.0" />
  </ItemGroup>
</Project>
